{
  "name": "genext",
  "version": "0.0.1",
  "scripts": {
    "dev": "next",
    "build": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx",
    "pretty-quick": "pretty-quick",
    "analyze": "cross-env ANALYZE=1 next build && cat .next/stats.txt",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-format lint build",
    "api:codegen": "graphql-codegen --config codegen-api.yml",
    "hasura:codegen": "graphql-codegen --config codegen-hasura.js"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@use-it/event-listener": "^0.1.3",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.4.7",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "cookie": "^0.4.0",
    "formik": "^2.0.10",
    "graphql-tag": "^2.10.1",
    "http-status": "^1.4.2",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.get": "^4.4.2",
    "next": "latest",
    "next-cookies": "^2.0.3",
    "ramda": "^0.26.1",
    "react": "16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "16.12.0",
    "react-helmet": "^5.2.1",
    "react-is": "^16.12.0",
    "react-spring": "^8.0.27",
    "react-ssr-prepass": "^1.0.8",
    "styled-components": "^4.4.1",
    "urql": "^1.7.0",
    "yup": "^0.28.0",
    "zustand": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@graphql-codegen/cli": "^1.9.1",
    "@graphql-codegen/introspection": "^1.9.1",
    "@graphql-codegen/typescript": "^1.9.1",
    "@graphql-codegen/typescript-operations": "^1.9.1",
    "@graphql-codegen/typescript-react-apollo": "^1.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/cookie": "^0.3.3",
    "@types/graphql": "^14.5.0",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^24.0.25",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "13.1.2",
    "@types/ramda": "^0.26.39",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/styled-components": "4.4.1",
    "@types/yup": "^0.26.27",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "apollo-link-token-refresh": "^0.2.7",
    "autoprefixer": "^9.7.3",
    "babel-jest": "^24.9.0",
